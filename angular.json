{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "performance-benifit-ui-shared": {
      "projectType": "library",
      "root": "libs/performance-benifit/ui-shared",
      "sourceRoot": "libs/performance-benifit/ui-shared/src",
      "prefix": "nx-sagacity",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/performance-benifit/ui-shared"],
          "options": {
            "jestConfig": "libs/performance-benifit/ui-shared/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/performance-benifit/ui-shared/src/**/*.ts",
              "libs/performance-benifit/ui-shared/src/**/*.html"
            ]
          }
        }
      }
    },
    "performance-benifits": {
      "projectType": "application",
      "root": "apps/performance-benifits",
      "sourceRoot": "apps/performance-benifits/src",
      "prefix": "nx-sagacity",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/performance-benifits",
            "index": "apps/performance-benifits/src/index.html",
            "main": "apps/performance-benifits/src/main.ts",
            "polyfills": "apps/performance-benifits/src/polyfills.ts",
            "tsConfig": "apps/performance-benifits/tsconfig.app.json",
            "assets": [
              "apps/performance-benifits/src/favicon.ico",
              "apps/performance-benifits/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/performance-benifits/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/performance-benifits/src/environments/environment.ts",
                  "with": "apps/performance-benifits/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "performance-benifits:build:production"
            },
            "development": {
              "browserTarget": "performance-benifits:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/performance-benifits/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "performance-benifits:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/performance-benifits/src/**/*.ts",
              "apps/performance-benifits/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/performance-benifits"],
          "options": {
            "jestConfig": "apps/performance-benifits/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "performance-benifits-e2e": {
      "root": "apps/performance-benifits-e2e",
      "sourceRoot": "apps/performance-benifits-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/performance-benifits-e2e/cypress.json",
            "devServerTarget": "performance-benifits:serve:development",
            "tsConfig": "apps/performance-benifits-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "performance-benifits:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/performance-benifits-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "performance-benifits-widget-app": {
      "projectType": "library",
      "root": "libs/performance-benifits/widget-app",
      "sourceRoot": "libs/performance-benifits/widget-app/src",
      "prefix": "nx-sagacity",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/performance-benifits/widget-app"],
          "options": {
            "jestConfig": "libs/performance-benifits/widget-app/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/performance-benifits/widget-app/src/**/*.ts",
              "libs/performance-benifits/widget-app/src/**/*.html"
            ]
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/performance-benifits/widget-app/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/performance-benifits-widget-app",
            "config": {
              "configFolder": "libs/performance-benifits/widget-app/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "performance-benifits-widget-app-e2e": {
      "root": "apps/performance-benifits-widget-app-e2e",
      "sourceRoot": "apps/performance-benifits-widget-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/performance-benifits-widget-app-e2e/cypress.json",
            "devServerTarget": "performance-benifits-widget-app:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "performance-benifits-widget-app:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "apps/performance-benifits-widget-app-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "store-ui-shared": {
      "projectType": "library",
      "root": "libs/store/ui-shared",
      "sourceRoot": "libs/store/ui-shared/src",
      "prefix": "nx-sagacity",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/store/ui-shared"],
          "options": {
            "jestConfig": "libs/store/ui-shared/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/store/ui-shared/src/**/*.ts",
              "libs/store/ui-shared/src/**/*.html"
            ]
          }
        }
      }
    },
    "util-interface": {
      "root": "libs/util-interface",
      "sourceRoot": "libs/util-interface/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/util-interface/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/util-interface"],
          "options": {
            "jestConfig": "libs/util-interface/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "7750123d-75ea-4b71-b9bf-70309090b2ed",
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "performance-benifits"
}
